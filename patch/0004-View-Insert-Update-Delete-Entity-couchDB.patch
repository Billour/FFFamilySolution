 using System.Collections.Generic;
 using System.Linq;
 using System.Text;
+using FFF.Core.CouchDB.Entity;
+using Microsoft.Http;
+using System.Net;
+using System.Runtime.Serialization.Json;
 
 namespace FFF.Core.CouchDB.Extensions
 {
     public static class CouchDBExtensions
     {
-        
+        public static T ToRestFulService<T>(this DBConfig config, string url, string id, string rev, T obj)
+        {
+            T returnData = default(T);
+            
+            HttpClient client = new HttpClient();
+
+            client.TransportSettings.Credentials = new NetworkCredential(config.UserID, config.UserPassword);
+
+            HttpContent content = HttpContentExtensions.CreateJsonDataContract<T>(obj, null);
+
+            HttpResponseMessage resp = client.Put(url, content);
+
+            HttpContent respContent = resp.Content;
+
+            returnData.GetType().GetProperty("StatusCode").SetValue(returnData, resp.StatusCode, null);
+
+            if (resp.StatusCode == HttpStatusCode.Created || resp.StatusCode == HttpStatusCode.OK)
+            {
+                returnData = respContent.ReadAsJsonDataContract<T>();
+            }
+            else
+            {
+                returnData.GetType().GetProperty("StatusMessage").SetValue(returnData, resp.Uri.Query + respContent.ReadAsString(), null);
+            }
+
+            return returnData;
+        }
     }
 }
